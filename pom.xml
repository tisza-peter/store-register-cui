  <artifactId>store-register-cui</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>store-register-gui</name>
  <properties>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
