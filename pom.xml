  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
    <artifactId>maven-jar-plugin</artifactId>
    <version>3.1.0</version>
    <configuration>
          <archive>
  <manifest>
    <mainClass>hu.lechnerkozpont.bootcamp.storage_ui.ConsoleApp</mainClass>
    </manifest>
    </archive>
        </configuration>
    </plugin>
    </plugins>
